{
    "name": "mohammedmanssour/fast-csv",
    "description": "Fast, Memory-light, CSV Importer/Exporter that provide better testing experience",
    "keywords": [
        "MohammedManssour",
        "fast-csv"
    ],
    "homepage": "https://github.com/mohammedmanssour/fast-csv",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Manssour",
            "email": "hello@mohammedmanssour.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "mikey179/vfsstream": "^1.6"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^11.1.3",
        "spatie/ray": "^1.28",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MohammedManssour\\FastCSV\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MohammedManssour\\FastCSV\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
